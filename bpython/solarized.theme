[syntax]
    keyword     = g
    name        = b
    comment     = d
    string      = c
    error       = R
    number      = c
    operator    = g
    punctuation = d
    token       = c
    paren       = b

[interface]
    background  = d
    output      = d
    main        = d
    prompt      = d
